{
  "name": "wiktionary-definition-parser",
  "description": "Parse a Wiktionary word's definitions",
  "main": "dist/wiktionary-definition-parser.js",
  "types": "types/wiktionary-definition-parser.d.ts",
  "scripts": {
    "lint": "eslint --ext=ts src test",
    "lint:fix": "eslint --ext=ts --fix src test",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "webpack --mode production",
    "preversion": "npm test",
    "version": "rm -rf dist && rm -rf types && npm run build",
    "postversion": "git push --tags"
  },
  "files": [
    "LICENSE",
    "README.md",
    "types"
  ],
  "keywords": [
    "wiktionary",
    "parse",
    "definition"
  ],
  "license": "MIT",
  "author": {
    "name": "Maroun Baydoun",
    "url": "https://www.maroun-baydoun.com/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:maroun-baydoun/wiktionary-definition-parser.git"
  },
  "bugs": {
    "url": "https://github.com/maroun-baydoun/wiktionary-definition-parser/issues"
  },
  "devDependencies": {
    "@types/jest": "24.0.25",
    "@typescript-eslint/eslint-plugin": "2.14.0",
    "@typescript-eslint/parser": "2.14.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-prettier": "3.1.2",
    "jest": "24.9.0",
    "prettier": "1.19.1",
    "ts-jest": "24.2.0",
    "ts-loader": "6.2.1",
    "typescript": "3.7.4",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js?|ts?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  },
  "dependencies": {},
  "version": "0.1.0"
}
