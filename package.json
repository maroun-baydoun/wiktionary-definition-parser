{
  "name": "wiktionary-definition-parser",
  "version": "0.3.0",
  "description": "Parse a Wiktionary word's definitions",
  "main": "dist/wiktionary-definition-parser.js",
  "types": "types/wiktionary-definition-parser.d.ts",
  "scripts": {
    "lint": "eslint --ext=ts src test",
    "lint:fix": "eslint --ext=ts --fix src test",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "webpack --mode production",
    "preversion": "npm test",
    "version": "rm -rf dist && rm -rf types && npm run build",
    "postversion": "git push --tags"
  },
  "files": [
    "LICENSE",
    "README.md",
    "types"
  ],
  "keywords": [
    "wiktionary",
    "parse",
    "definition"
  ],
  "license": "MIT",
  "author": {
    "name": "Maroun Baydoun",
    "url": "https://maroun-baydoun.com/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:maroun-baydoun/wiktionary-definition-parser.git"
  },
  "bugs": {
    "url": "https://github.com/maroun-baydoun/wiktionary-definition-parser/issues"
  },
  "devDependencies": {
    "@types/jest": "26.0.10",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-prettier": "3.1.4",
    "jest": "26.4.2",
    "prettier": "2.1.1",
    "ts-jest": "26.3.0",
    "ts-loader": "8.0.3",
    "typescript": "4.0.2",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js?|ts?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  },
  "dependencies": {}
}
